{
  "expo": {
    "name": "TSM_APP",
    "slug": "tsm-app",
    "scheme": "tsmapp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "android": {
      "package": "com.yourcompany.tsmapp",
      "intentFilters": [
        {
          "action": "VIEW",
          "data": [
            {
              "scheme": "tsmapp"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ]
    }
  }
}